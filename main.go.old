package main

import (
	"fmt"

	"github.com/pseudo-su/templ"
	"github.com/pseudo-su/templ/tree"
)

func main() {
	params := map[string]string{
		"param1": "param1value",
		"param2": "param2value",
	}

	templ := templ.New().Params(params).File("test/fixtures/simple.yaml")
	t, err := templ.Execute()

	if err != nil {
		fmt.Println("ERR:", err)
		return
	}
	desc, _ := tree.DescribeTree(t, nil)
	fmt.Println(desc)
}
